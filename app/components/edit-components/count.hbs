<Criteria::SelectCriterion
  @value={{this.value}}
  @treeNode={{@node}}
  as |label check score|>
  <Labels::NumericString @label={{label}} @value={{this.value}} as |string|>
    {{#if (or label @etns.hasComment)}}
      <AuCriteriaInline @node={{@node}} @criterium={{string}} @score={{score}} @etns={{@etns}} @node={{@node}} />
    {{/if}}
  </Labels::NumericString>
</Criteria::SelectCriterion>
