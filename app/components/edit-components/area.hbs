{{#let (array this.value.width this.value.height) as |arrValue|}}
  <Criteria::SelectAreaCriterion
    @value={{arrValue}}
    @treeNode={{@node}}
    as |label check|>
    {{#if label}}
      <AuCriteriaInline @criterium={{@node.title}} @check={{check}}>
        <strong><Labels::AreaString @label={{label}} @value={{arrValue}} /></strong>
      </AuCriteriaInline>
    {{/if}}
  </Criteria::SelectAreaCriterion>
{{/let}}