<Criteria::SelectCriterion
  @value={{this.value}}
  @treeNode={{@node}}
  as |label check score|>
  {{#if label}}
    <Labels::NumericString @label={{label}} @value={{this.value}} as |string|>
      <AuCriteriaInline @criterium={{string}} @score={{score}} @comment={{@comment}} />
    </Labels::NumericString>
  {{/if}}
</Criteria::SelectCriterion>