              <AuCriteriaSubitem @criterium="Museumpas" @check="true">
              </AuCriteriaSubitem>
              <AuCriteriaSubitem @criterium="Uitpas (punten scannen)" @check="true">
              </AuCriteriaSubitem>
              <AuCriteriaSubitem @criterium="City Pas Gent" @check="true">
              </AuCriteriaSubitem>
            </ul>
          </AuCriteriaItem>
          <AuCriteriaItem @criterium="Rolstoel ter beschikking" @check="true">
            <p>Ja, 2 reserveren via <a href="mailto:info@stamgent.be">info@stamgent.be</a></p>
          </AuCriteriaItem>
          <AuCriteriaItem @criterium="Wifi niet beschikbaar">
          </AuCriteriaItem>
          <AuCriteriaItem @criterium="Geen voorzieningen voor assistentiehonden">
          </AuCriteriaItem>
          <AuCriteriaItem @criterium="Elektronische betaalmogelijkheid niet verplaatsbaar">
          </AuCriteriaItem>
          <AuCriteriaItem @criterium="Website" @check="true">
            <ul class="au-w-criterium-list">
              <AuCriteriaSubitem @criterium="Tekstvergroting mogelijk" @check="true">
              </AuCriteriaSubitem>
              <AuCriteriaSubitem @criterium="Contrasten" @check="true">
              </AuCriteriaSubitem>
            </ul>
          </AuCriteriaItem>
        </ul>
      </AuModal>
      <AuModal @title="Inkom" @id="{{@id}}inkom" @check="true" @theme="{{@theme}}">
