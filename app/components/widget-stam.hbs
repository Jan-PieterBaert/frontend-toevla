      <span class="au-w-header__rating">{{@category}}</span> {{#if @rating}}<span class="au-w-header__rating au-w-header__rating--base">{{@rating}}</span>{{/if}}
    </p>
    {{/if}}
  </div>
  <ul class="au-w-grid au-w-grid--divider au-u-margin-bottom-small">
    <li class="au-w-grid__item">
      <h2 class="au-w-hidden-visually">Om er te geraken</h2>
      <AuModal @title="Openbaar vervoer" @id="{{@id}}openbaar-vervoer" @theme="{{@theme}}">
        <p class="au-w-info">
          Deze locatie is bereikbaar met het openbaar vervoer.
        </p>
        <ul class="au-w-criterium-list">
          <AuCriteriaItem @criterium="Halte aanwezig binnen 500 m" @check="true">
            <ul>
              <li>Bus: De Lijn, nr 6, tpv Bijlokehof op 350 m</li>
              <li>ram: 2 en 4, afstand 400 m</li>
            </ul>
          </AuCriteriaItem>
          <AuCriteriaItem @criterium="Toegankelijke halte" @info="true">
            <p><a href="https://www.delijn.be/nl/routeplanner/">De Lijn routeplanner</a></p>
          </AuCriteriaItem>
          <AuCriteriaItem @criterium="Rolstoelvriendelijk pad naar inkom">
            <ul class="au-w-criterium-list">
              <AuCriteriaSubitem @criterium="Slechte ondergrond">
                <p>Kasseien aan bushalte</p>
              </AuCriteriaSubitem>
              <AuCriteriaSubitem @criterium="Smalste punt op de route: 70 cm">
                <p>Fietsenstallingen versmallen de route</p>
              </AuCriteriaSubitem>
              <AuCriteriaSubitem @criterium="Drempel van meer dan 2 cm, nml 3 cm">
                Er zijn 2 drempels (boordstenen) van 2 cm
              </AuCriteriaSubitem>
            </ul>
          </AuCriteriaItem>
        </ul>
      </AuModal>
