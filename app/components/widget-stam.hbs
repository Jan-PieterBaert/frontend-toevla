        <ul class="au-w-criterium-list">
          <AuCriteriaItem @criterium="Rolstoelvriendelijke inkom" @check="true">
            <ul class="au-w-criterium-list">
              <AuCriteriaSubitem @criterium="Bereikbaar" @check="true">
              </AuCriteriaSubitem>
              <AuCriteriaSubitem @criterium="Geen drempel" @check="true">
              </AuCriteriaSubitem>
              <AuCriteriaSubitem @criterium="Deurbreedte 155 cm" @check="true">
              </AuCriteriaSubitem>
              <AuCriteriaSubitem @criterium="Draairuimte aan deur 150 cm" @check="true">
